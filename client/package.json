{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-bootstrap": "^0.32.15",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "final-form": "^4.11.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-final-form": "^4.0.2",
    "react-loading": "^2.0.3",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-graphql": "cd ../ && eslint -c .eslintrc-graphql --no-eslintrc --ext ts --ext tsx client/src/",
    "clear": "find . -name \"__generated__\" -exec rm -rf '{}' +",
    "apollo-codegen": "apollo client:codegen --addTypename --target=typescript --localSchemaFile=../server/src/schema/__generated__/schema.graphql --includes=\"./src/**/*.{ts,tsx}\" --tagName gql",
    "watch": "npm run clear && concurrently -k npm:watch-*",
    "watch-app": "npm run start",
    "watch-apollo-codegen": "npm run apollo-codegen -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/history": "^4.7.2",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.1",
    "graphql-code-generator": "^0.15.2",
    "graphql-codegen-add": "^0.15.2",
    "graphql-codegen-fragment-matcher": "^0.15.2",
    "graphql-codegen-typescript-client": "^0.15.2",
    "graphql-codegen-typescript-common": "^0.15.2",
    "graphql-codegen-typescript-react-apollo": "^0.15.2",
    "mkdirp": "^0.5.1",
    "typescript": "3.2.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
